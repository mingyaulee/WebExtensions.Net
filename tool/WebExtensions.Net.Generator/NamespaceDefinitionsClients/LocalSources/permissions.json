[
  {
    "namespace": "permissions",
    "types": [
      {
        "id": "Permissions",
        "type": "object",
        "properties": {
          "permissions": {
            "optional": true,
            "type": "array",
            "items": {
              "choices": [
                {
                  "$ref": "manifest.OptionalPermission"
                },
                {
                  "$ref": "manifest.OptionalOnlyPermission"
                }
              ]
            }
          },
          "origins": {
            "optional": true,
            "type": "array",
            "items": {
              "$ref": "manifest.MatchPattern"
            }
          },
          "data_collection": {
            "optional": true,
            "type": "array",
            "items": {
              "$ref": "manifest.OptionalDataCollectionPermission"
            }
          }
        }
      },
      {
        "id": "AnyPermissions",
        "type": "object",
        "properties": {
          "permissions": {
            "optional": true,
            "type": "array",
            "items": {
              "choices": [
                {
                  "$ref": "manifest.Permission"
                },
                {
                  "$ref": "manifest.OptionalOnlyPermission"
                }
              ]
            }
          },
          "origins": {
            "optional": true,
            "type": "array",
            "items": {
              "$ref": "manifest.MatchPattern"
            }
          },
          "data_collection": {
            "optional": true,
            "type": "array",
            "items": {
              "$ref": "manifest.OptionalDataCollectionPermission"
            }
          }
        }
      }
    ],
    "functions": [
      {
        "name": "getAll",
        "description": "Get a list of all the extension\u0027s permissions.",
        "async": "callback",
        "type": "function",
        "parameters": [
          {
            "name": "callback",
            "type": "function",
            "parameters": [
              {
                "name": "permissions",
                "$ref": "AnyPermissions"
              }
            ]
          }
        ]
      },
      {
        "name": "contains",
        "description": "Check if the extension has the given permissions.",
        "async": "callback",
        "type": "function",
        "parameters": [
          {
            "name": "permissions",
            "$ref": "AnyPermissions"
          },
          {
            "name": "callback",
            "type": "function",
            "parameters": [
              {
                "name": "result",
                "type": "boolean"
              }
            ]
          }
        ]
      },
      {
        "name": "request",
        "description": "Request the given permissions.",
        "async": "callback",
        "type": "function",
        "parameters": [
          {
            "name": "permissions",
            "$ref": "Permissions"
          },
          {
            "name": "callback",
            "type": "function",
            "parameters": [
              {
                "name": "granted",
                "type": "boolean"
              }
            ]
          }
        ]
      },
      {
        "name": "remove",
        "description": "Relinquish the given permissions.",
        "async": "callback",
        "type": "function",
        "parameters": [
          {
            "name": "permissions",
            "$ref": "Permissions"
          },
          {
            "name": "callback",
            "type": "function",
            "parameters": []
          }
        ]
      }
    ],
    "events": [
      {
        "name": "onAdded",
        "description": "Fired when the extension acquires new permissions.",
        "type": "function",
        "parameters": [
          {
            "name": "permissions",
            "$ref": "Permissions"
          }
        ]
      },
      {
        "name": "onRemoved",
        "description": "Fired when permissions are removed from the extension.",
        "type": "function",
        "parameters": [
          {
            "name": "permissions",
            "$ref": "Permissions"
          }
        ]
      }
    ]
  }
]